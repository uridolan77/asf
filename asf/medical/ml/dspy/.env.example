# DSPy Settings

# LLM Provider Settings
DSPY_LLM_PROVIDER=openai
DSPY_LLM_API_KEY=your-api-key-here
DSPY_DEFAULT_MODEL=gpt-3.5-turbo
DSPY_FALLBACK_MODEL=gpt-3.5-turbo-instruct

# Model Parameters
DSPY_MAX_TOKENS=1000
DSPY_TEMPERATURE=0.7
DSPY_TOP_P=1.0

# Caching Settings
DSPY_CACHE_BACKEND=disk
DSPY_CACHE_DIRECTORY=.dspy_cache
DSPY_CACHE_TTL=3600
# DSPY_REDIS_URL=redis://localhost:6379/0
# DSPY_REDIS_PASSWORD=your-redis-password

# Telemetry Settings
DSPY_ENABLE_TELEMETRY=true
DSPY_LOG_ASYNC=true
DSPY_MLFLOW_EXPERIMENT_NAME=dspy-experiments

# Retry Settings
DSPY_MAX_RETRIES=3
DSPY_RETRY_MIN_WAIT=1.0
DSPY_RETRY_MAX_WAIT=10.0

# Thread Settings
DSPY_THREAD_LIMIT=4

# Medical Research Specific Settings
DSPY_ENABLE_PHI_DETECTION=true
DSPY_AUDIT_LOGGING=true
DSPY_AUDIT_LOG_PATH=audit_logs
